<?xml version='1.0' encoding='utf-8'?>
<widget id="com.app.torrance" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Torrance</name>
    <description>
        A conference application for refineries
    </description>
    <author email="admin@torrance.com" href="http://www.torrance.com">
        Torrance
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <allow-intent href="*" />
    <access launch-external="yes" origin="*" />
    <access launch-external="yes" origin="http://torrance.fitandappy.com/*" />
    <access origin="http://fitandappy.com" subdomains="true" />
    <access origin="http://torrance.fitandappy.com" />
    <access origin="http://torrance.fitandappy.com/*" />
    <access origin="http://torrance.fitandappy.com/index.php" />
    <access origin="http://torrance.fitandappy.com/android" />
    <access origin="http://torrance.fitandappy.com/admin_cta/vehicle_upload/Vehicle%20Request%20Form.pdf/*" />
    <allow-intent href="http://torrance.fitandappy.com/admin_cta/vehicle_upload/Vehicle%20Request%20Form.pdf" />
    <allow-intent href="http://torrance.fitandappy.com" />
    <allow-navigation href="*://maps.google.com/maps" />
    <allow-navigation href="*://www.google.com/maps" />
    <access-navigation href="http://torrance.fitandappy.com" />
    <access-navigation href="http://torrance.fitandappy.com/*" />
    <access-navigation href="http://torrance.fitandappy.com/index.php" />
    <access-navigation href="http://torrance.fitandappy.com/admin_cta/vehicle_upload/Vehicle%20Request%20Form.pdf" />
    <allow-navigation href="http://torrance.fitandappy.com/*" />
    <allow-navigation href="*" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="loadUrlTimeoutValue" value="60000" />
</widget>
